<h3>Sign-In,
        <%= @user.username %>


<%= form_tag('/log_in', method: :post) do %>
  <%= label_tag :username %>
  <%= text_field_tag :username %>
  <%= label_tag :password %>
  <%= password_field_tag :password %>
  <%= submit_tag "Submit" %>
<% end %>

<%= form_tag('/new', method: :get) do %>
<%= submit_tag "New users click here " %>
<% end %>
